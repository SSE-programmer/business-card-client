{
    "plugins": [
        "html"
    ],
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-trailing-spaces": "off",
        "@typescript-eslint/no-non-null-assertion": "off"
    }
}
